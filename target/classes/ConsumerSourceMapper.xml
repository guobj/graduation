<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.gbj.mapper.ConsumerSourceMapper">
    <resultMap type="com.gbj.model.ConsumerSource" id="ConsumerSourceBaseResultMap">
        <id column="cs_id" property="cs_id"/>
        <result column="cs_name" property="cs_name"/>
    </resultMap>
    <select id="load" parameterType="integer" resultMap="ConsumerSourceBaseResultMap">
        SELECT * FROM consumer_source WHERE cs_id=#{cs_id}
    </select>
    <select id="consumerSourceList" resultType="com.gbj.model.ConsumerSource">
        SELECT 
            * 
        FROM 
            consumer_source
    </select>
  </mapper>